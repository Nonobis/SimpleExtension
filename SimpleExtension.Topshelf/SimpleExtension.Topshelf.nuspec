    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <releaseNotes>Refactoring</releaseNotes>
    <copyright>Copyright Arnaud Dartois 2016-2017</copyright>
    <tags>Topshelf Extension Collection</tags>
  </metadata>
  <files>
    <file src="*.*" target="lib\net45\" exclude="*.pdb;*.nupkg;*.vshost.*"/>
    <file src="**\*.*" target="lib\net45\" exclude="*.pdb;*.nupkg;*.vshost.*"/>
  </files>
</package>